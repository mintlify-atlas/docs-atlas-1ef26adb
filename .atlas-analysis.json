  "keyFeatures": [
    "Semantic, fulltext, BM25, and hybrid search with advanced filtering",
    "RAG (Retrieval Augmented Generation) with customizable LLM integration",
    "Recommendations engine based on text similarity",
    "Comprehensive analytics and A/B testing with experiments",
    "Multi-tenant datasets with organization and user management",
    "File upload with automatic chunking (PDF, DOCX, TXT, CSV, JSONL, HTML)",
    "Group-based organization for related chunks",
    "Dataset crawling and site search components",
    "Self-hostable via Docker Compose or cloud platforms",
    "Vector inference service for fast, low-latency embeddings",
    "Customizable embedding models and reranker models",
    "OpenAPI compatibility for vector inference"
  ],
  "publicApiSurface": [
    "POST /api/chunk - Create or upsert chunks",
    "POST /api/chunk/search - Search chunks with semantic, fulltext, BM25, or hybrid modes",
    "POST /api/chunk/autocomplete - Get autocomplete suggestions",
    "POST /api/chunk/recommend - Get chunk recommendations",
    "POST /api/chunk/generate - RAG on specified chunks",
    "POST /api/chunk_group - Create or upsert chunk groups",
    "POST /api/chunk_group/search - Search over groups",
    "POST /api/chunk_group/group_oriented_search - Search within a specific group",
    "POST /api/file - Upload files for automatic chunking",
    "POST /api/dataset - Create datasets",
    "GET /api/dataset/{dataset_id} - Get dataset details",
    "POST /api/analytics/search - Get search analytics",
    "POST /api/analytics/rag - Get RAG analytics",
    "POST /api/analytics/events - Send user event data",
    "POST /api/experiment - Create A/B test experiments",
    "POST /api/crawl - Create dataset crawl requests",
    "POST /api/organization - Manage organizations",
    "POST /api/topic - Create RAG conversation topics",
    "POST /api/message - Create RAG messages",
    "POST /embed - Generate embeddings (Vector Inference)",
    "POST /rerank - Rerank search results (Vector Inference)",
    "POST /v1/embeddings - OpenAI-compatible embeddings endpoint"
  ]
